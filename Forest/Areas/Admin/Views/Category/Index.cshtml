@model IEnumerable<Category>


<table class="table table-dark">
    <thead>
        <tr>
            <th>#</th>
            <th>Category Name</th>
            <th><a href="/admin/category/Create" class="btn btn-success w-50">Create</a></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.CategoryName</td>
                <td>
                    <a href="/admin/category/edit/@item.Id" class="btn btn-primary">Edit</a>
                    <a href="/admin/category/delete/@item.Id" class="btn btn-danger">Delete</a>
                    <a href="/admin/category/detail/@item.Id" class="btn btn-info">Detail</a>
                </td>
            </tr>
        }
    </tbody>
</table>